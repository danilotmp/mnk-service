        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set(\"accessToken\", jsonData.data.accessToken);",
                  "    pm.collectionVariables.set(\"refreshToken\", jsonData.data.refreshToken);",
                  "    pm.collectionVariables.set(\"userId\", jsonData.data.user.id);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Accept-Language",
                "value": "es",
                "description": "Idioma: es, en, pt"
              },
              {
                "key": "company-code",
                "value": "MNK",
                "description": "Código de la empresa (multiempresa)"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@mnksolutions.com\",\n  \"password\": \"Admin123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/seguridades/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "seguridades", "auth", "login"]
            },
            "description": "Iniciar sesión y obtener tokens JWT\n\n**Credenciales de Prueba:**\n- Admin: admin@mnksolutions.com / Admin123!\n- Usuario: test@mnksolutions.com / Test123!"
          },
          "response": []
        },
        {
